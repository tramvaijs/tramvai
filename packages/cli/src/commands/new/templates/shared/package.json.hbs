{
  {{#if isMonorepo}}
    "name": "{{configEntry.name}}-monorepo",
  {{else}}
    "name": "{{configEntry.name}}",
  {{/if}}
  "private": true,
  "scripts": {
    "start": "tramvai start {{configEntry.name}}",
    "build": "tramvai build {{configEntry.name}}",
    "preview": "tramvai start-prod {{configEntry.name}}",
    "analyze": "tramvai {{configEntry.name}} --analyze=statoscope",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore ."{{#if isJest}},
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:integration": "playwright test"{{/if}}
  },
  "devDependencies": {},
  "dependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }{{#if workspaceBaseDir}},
  "workspaces": ["{{workspaceBaseDir}}/*"]{{/if}}
}
