{
  "$schema": "../../node_modules/@tramvai/cli/schema.json",
  "projects": {
    "integrity": {
      "name": "integrity",
      "root": "src",
      "modern": false,
      "type": "application",
      "integrity": true,
      "fileSystemPages": {
        "enabled": true,
        "pagesDir": false
      },
      "splitChunks": {
        "mode": "granularChunks"
      },
      "sourceMap": {
        "development": false,
        "production": true
      }
    }
  }
}
