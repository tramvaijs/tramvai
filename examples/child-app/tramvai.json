{
  "$schema": "../../node_modules/@tramvai/cli/schema.json",
  "analytics": {
    "enabled": false
  },
  "projects": {
    "root-app": {
      "name": "root-app",
      "root": "root-app",
      "type": "application",
      "hotRefresh": {
        "enabled": true
      },
      "shared": {
        "criticalChunks": ["bootstrap"],
        "deps": [
          "@tramvai/react-query",
          "@tramvai/module-react-query"
        ]
      },
      "sourceMap": {
        "development": false,
        "production": true
      },
      "experiments": {
        "transpilation": {
          "loader": "swc",
          "include": [
            "@tinkoff/request",
            "@tramvai/module-dev-tools",
            "@tramvai-tinkoff/module-push-web"
          ]
        }
      },
      "webpack": {
        "devtool": "eval-cheap-module-source-map"
      }
    },
    "base": {
      "name": "base",
      "root": "child-apps/base",
      "type": "child-app",
      "hotRefresh": {
        "enabled": true
      }
    },
    "client-hints": {
      "name": "client-hints",
      "root": "child-apps/client-hints",
      "type": "child-app"
    },
    "commandline": {
      "name": "commandline",
      "root": "child-apps/commandline",
      "type": "child-app"
    },
    "error": {
      "name": "error",
      "root": "child-apps/error",
      "type": "child-app"
    },
    "header": {
      "name": "header",
      "root": "child-apps/header",
      "type": "child-app"
    },
    "react-query": {
      "name": "react-query",
      "root": "child-apps/react-query",
      "type": "child-app",
      "shared": {
        "deps": [
          "@tramvai/react-query",
          "@tramvai/module-react-query"
        ]
      }
    },
    "router": {
      "name": "router",
      "root": "child-apps/router",
      "type": "child-app"
    },
    "state": {
      "name": "state",
      "root": "child-apps/state",
      "type": "child-app"
    },
    "loadable": {
      "name": "loadable",
      "root": "child-apps/loadable",
      "type": "child-app"
    },
    "contracts": {
      "name": "contracts",
      "root": "child-apps/contracts",
      "type": "child-app"
    }
  }
}